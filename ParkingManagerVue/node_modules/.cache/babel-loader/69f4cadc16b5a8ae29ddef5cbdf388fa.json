{"remainingRequest":"F:\\IdeaProjects\\ParkingManagerVue\\node_modules\\babel-loader\\lib\\index.js!F:\\IdeaProjects\\ParkingManagerVue\\node_modules\\cache-loader\\dist\\cjs.js??ref--1-0!F:\\IdeaProjects\\ParkingManagerVue\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!F:\\IdeaProjects\\ParkingManagerVue\\src\\components\\mane\\UserMane.vue?vue&type=script&lang=js","dependencies":[{"path":"F:\\IdeaProjects\\ParkingManagerVue\\src\\components\\mane\\UserMane.vue","mtime":1696068366447},{"path":"F:\\IdeaProjects\\ParkingManagerVue\\babel.config.js","mtime":1616250210485},{"path":"F:\\IdeaProjects\\ParkingManagerVue\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1720454233480},{"path":"F:\\IdeaProjects\\ParkingManagerVue\\node_modules\\babel-loader\\lib\\index.js","mtime":1720454234267},{"path":"F:\\IdeaProjects\\ParkingManagerVue\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1720454233480},{"path":"F:\\IdeaProjects\\ParkingManagerVue\\node_modules\\vue-loader\\lib\\index.js","mtime":1720454234585}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"names":["axios","data","checkUsername","rule","value","callback","Error","setTimeout","get","then","res","checkAge","Number","isInteger","total","loading","queryInfo","username","nike","card","pagenum","pageSize","users","editUser","password","sex","age","phone","money","registerUser","addShow","editShow","editRules","niek","required","message","trigger","min","max","validator","registerRules","feeUserShow","feeUserRules","feeUser","methods","feeUserCon","formName","$refs","validate","valid","post","flag","$message","success","getList","error","msg","records","handleSizeChange","newSize","handleCurrentChange","newPage","addUser","item","editClose","$nextTick","resetFields","toEdit","user","uid","editCommit","userDelete","re","$confirm","confirmButtonText","cancelButtonText","type","catch","err","info","rePassword","addClose","submitForm","mounted"],"sources":["src/components/mane/UserMane.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n            <el-breadcrumb-item><i\r\n                    class=\"iconfont icon-r-user1\"\r\n                    style=\"margin: 5px; font-size: 22px\"\r\n                >\r\n                    用户信息管理</i></el-breadcrumb-item>\r\n        </el-breadcrumb>\r\n        <!-- 卡片视图区域 -->\r\n        <el-card>\r\n            <el-row :gutter=\"20\">\r\n                <el-col :span=\"4\">\r\n                    <el-input\r\n                        placeholder=\"请输入用户名\"\r\n                        v-model=\"queryInfo.username\"\r\n                        clearable\r\n                    ></el-input>\r\n                </el-col>\r\n                <el-col :span=\"4\">\r\n                    <el-input\r\n                        placeholder=\"请输入姓名\"\r\n                        v-model=\"queryInfo.nike\"\r\n                        clearable\r\n                    ></el-input>\r\n                </el-col>\r\n                <el-col :span=\"4\">\r\n                    <el-input\r\n                        placeholder=\"请输入车牌号\"\r\n                        v-model=\"queryInfo.card\"\r\n                        clearable\r\n                    ></el-input>\r\n                </el-col>\r\n                <el-col :span=\"4\">\r\n                    <el-button\r\n                        type=\"primary\"\r\n                        style=\"font-size: 18px\"\r\n                        @click=\"getList\"\r\n                    ><i class=\"iconfont icon-r-find\"  style=\"font-size: 22px;\"></i> \r\n                        搜索</el-button\r\n                    >\r\n                </el-col>\r\n                <el-col :span=\"4\">\r\n                    <el-button\r\n                        type=\"success\"\r\n                        style=\"font-size: 18px\"\r\n                        @click=\"addUser\"\r\n                    ><i class=\"iconfont icon-r-add\"  style=\"font-size: 22px;\"></i> \r\n                        新增</el-button\r\n                    >\r\n                </el-col>\r\n                <el-col :span=\"4\">\r\n                    <el-button\r\n                        type=\"warning\"\r\n                        style=\"font-size: 18px\"\r\n                        @click=\"feeUserShow = true\"\r\n                    ><i class=\"iconfont icon-r-shield\"  style=\"font-size: 22px;\"></i> \r\n                        充值</el-button\r\n                    >\r\n                </el-col>\r\n            </el-row>\r\n\r\n            <!-- 车位信息区域 -->\r\n            <el-table v-loading=\"loading\" :data=\"users\" border stripe>\r\n                <el-table-column\r\n                    width=\"50\"\r\n                    label=\"序号\"\r\n                    align=\"center\"\r\n                    type=\"index\"\r\n                ></el-table-column>\r\n                <el-table-column label=\"用户名\" prop=\"username\" align=\"center\">\r\n                </el-table-column>\r\n                <el-table-column label=\"姓名\" prop=\"nike\" align=\"center\">\r\n                </el-table-column>\r\n                <el-table-column label=\"性别\" prop=\"sex\" align=\"center\">\r\n                </el-table-column>\r\n                <el-table-column label=\"联系电话\" prop=\"phone\" align=\"center\">\r\n                </el-table-column>\r\n                <el-table-column label=\"车牌号\" prop=\"card\" align=\"center\">\r\n                </el-table-column>\r\n                <el-table-column label=\"账户余额\" prop=\"money\" align=\"center\">\r\n                    <template slot-scope=\"scope\">\r\n                        <span v-if=\"scope.row.money\"\r\n                            >{{ scope.row.money }}元</span\r\n                        >\r\n                        <span v-else>0元</span>\r\n                    </template>\r\n                </el-table-column>\r\n                <el-table-column label=\"操作\" align=\"center\" fixed=\"right\" width=\"200\">\r\n                    <template slot-scope=\"scope\">\r\n                        <el-button\r\n                            v-if=\"scope.row.username != 'admin'\"\r\n                            @click=\"toEdit(scope.row)\"\r\n                            type=\"primary\"\r\n                            circle\r\n                        ><i class=\"iconfont icon-r-edit\" style=\"font-size: 24px\"></i></el-button>\r\n                        <el-button\r\n                            v-if=\"scope.row.username != 'admin'\"\r\n                            @click=\"rePassword(scope.row.uid)\"\r\n                            type=\"warning\"\r\n                            circle\r\n                        ><i class=\"iconfont icon-r-lock\" style=\"font-size: 24px\"></i></el-button>\r\n                        <el-button\r\n                            v-if=\"scope.row.username != 'admin'\"\r\n                            @click=\"userDelete(scope.row.uid)\"\r\n                            type=\"danger\"\r\n                            circle\r\n                        ><i class=\"iconfont icon-r-delete\" style=\"font-size: 24px\"></i></el-button>\r\n                        <el-button\r\n                            v-if=\"scope.row.username == 'admin'\"\r\n                            @click=\"toEdit(scope.row)\"\r\n                            type=\"primary\"\r\n                            circle\r\n                            disabled\r\n                        ><i class=\"iconfont icon-r-edit\" style=\"font-size: 24px\"></i></el-button>\r\n                        <el-button\r\n                            disabled\r\n                            v-if=\"scope.row.username == 'admin'\"\r\n                            @click=\"userDelete(scope.row.uid)\"\r\n                            type=\"danger\"\r\n                            circle\r\n                        ><i class=\"iconfont icon-r-delete\" style=\"font-size: 24px\"></i></el-button>\r\n                    </template>\r\n                </el-table-column>\r\n            </el-table>\r\n\r\n            <el-pagination\r\n                @size-change=\"handleSizeChange\"\r\n                @current-change=\"handleCurrentChange\"\r\n                :current-page=\"queryInfo.pagenum\"\r\n                :page-sizes=\"[5, 10, 15, 20]\"\r\n                :page-size=\"queryInfo.pageSize\"\r\n                layout=\"total, sizes, prev, pager, next\"\r\n                :total=\"total\"\r\n            >\r\n            </el-pagination>\r\n        </el-card>\r\n\r\n        <el-dialog\r\n            title=\"修改用户\"\r\n            :visible.sync=\"editShow\"\r\n            width=\"450px\"\r\n            center\r\n            @close=\"editClose\"\r\n        >\r\n            <!-- 内容主题区域 -->\r\n            <el-form\r\n                :model=\"editUser\"\r\n                :rules=\"editRules\"\r\n                ref=\"editRules\"\r\n                label-width=\"80px\"\r\n            >\r\n                <el-form-item label=\"姓名\" prop=\"nike\" class=\"is-required\">\r\n                    <el-input\r\n                        v-model=\"editUser.nike\"\r\n                        style=\"width: 300px\"\r\n                    ></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"密码\" prop=\"password\" class=\"is-required\">\r\n                    <el-input\r\n                        v-model=\"editUser.password\"\r\n                        show-password\r\n                        style=\"width: 300px\"\r\n                    ></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"余额\" prop=\"money\" class=\"is-required\">\r\n                    <el-input\r\n                        v-model.number=\"editUser.money\"\r\n                        style=\"width: 300px\"\r\n                    ></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"联系电话\" prop=\"phone\" class=\"is-required\">\r\n                    <el-input\r\n                        v-model=\"editUser.phone\"\r\n                        style=\"width: 300px\"\r\n                    ></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"车牌号\" prop=\"card\" class=\"is-required\">\r\n                    <el-input\r\n                        v-model=\"editUser.card\"\r\n                        style=\"width: 300px\"\r\n                    ></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"性别\" class=\"is-required\">\r\n                    <template>\r\n                        <el-radio v-model=\"editUser.sex\" label=\"男\"\r\n                            >男</el-radio\r\n                        >\r\n                        <el-radio v-model=\"editUser.sex\" label=\"女\"\r\n                            >女</el-radio\r\n                        >\r\n                    </template>\r\n                </el-form-item>\r\n                <el-form-item label=\"年龄\" prop=\"age\" class=\"is-required\">\r\n                    <el-input\r\n                        v-model.number=\"editUser.age\"\r\n                        style=\"width: 300px\"\r\n                    ></el-input>\r\n                </el-form-item>\r\n            </el-form>\r\n\r\n            <!-- 底部区域 -->\r\n            <span slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"editClose()\" style=\"font-size: 18px;\"><i class=\"iconfont icon-r-left\" style=\"font-size: 22px;\"></i> 取消</el-button>\r\n                <el-button type=\"primary\" @click=\"editCommit('editRules')\"\r\n                    style=\"font-size: 18px;\"><i class=\"iconfont icon-r-yes\" style=\"font-size: 22px;\"></i> 确认修改</el-button\r\n                >\r\n            </span>\r\n        </el-dialog>\r\n        <el-dialog\r\n            title=\"新增用户\"\r\n            :visible.sync=\"addShow\"\r\n            width=\"500px\"\r\n            center\r\n            @close=\"addClose\"\r\n        >\r\n            <!-- 内容主题区域 -->\r\n            <el-form\r\n                :model=\"registerUser\"\r\n                status-icon\r\n                :rules=\"registerRules\"\r\n                ref=\"registerRules\"\r\n                label-width=\"100px\"\r\n                class=\"demo-ruleForm\"\r\n            >\r\n                <el-form-item\r\n                    label=\"用户名\"\r\n                    prop=\"username\"\r\n                    class=\"is-required\"\r\n                >\r\n                    <el-input\r\n                        v-model=\"registerUser.username\"\r\n                        placeholder=\"请输入用户名\"\r\n                        style=\"width: 300px\"\r\n                    ></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"密码\" prop=\"password\" class=\"is-required\">\r\n                    <el-input\r\n                        v-model=\"registerUser.password\"\r\n                        placeholder=\"请输入密码\"\r\n                        show-password\r\n                        style=\"width: 300px\"\r\n                    ></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"姓名\" prop=\"nike\" class=\"is-required\">\r\n                    <el-input\r\n                        v-model=\"registerUser.nike\"\r\n                        placeholder=\"请输入姓名\"\r\n                        style=\"width: 300px\"\r\n                    ></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"性别\" prop=\"sex\" class=\"is-required\">\r\n                    <el-radio v-model=\"registerUser.sex\" label=\"男\"\r\n                        >男</el-radio\r\n                    >\r\n                    <el-radio v-model=\"registerUser.sex\" label=\"女\"\r\n                        >女</el-radio\r\n                    >\r\n                </el-form-item>\r\n                <el-form-item label=\"年龄\" prop=\"age\" class=\"is-required\">\r\n                    <el-input\r\n                        v-model.number=\"registerUser.age\"\r\n                        style=\"width: 300px\"\r\n                    ></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"车牌号\" prop=\"card\" class=\"is-required\">\r\n                    <el-input\r\n                        v-model=\"registerUser.card\"\r\n                        placeholder=\"请输入车牌号\"\r\n                        style=\"width: 300px\"\r\n                    ></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"联系电话\" prop=\"phone\" class=\"is-required\">\r\n                    <el-input\r\n                        v-model=\"registerUser.phone\"\r\n                        placeholder=\"请输入联系电话\"\r\n                        style=\"width: 300px\"\r\n                    ></el-input>\r\n                </el-form-item>\r\n            </el-form>\r\n\r\n            <!-- 底部区域 -->\r\n            <span slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"addClose()\" style=\"font-size: 18px;\"><i class=\"iconfont icon-r-left\" style=\"font-size: 22px;\"></i> 取消</el-button>\r\n                <el-button type=\"primary\" @click=\"submitForm('registerRules')\"\r\n                    style=\"font-size: 18px;\"><i class=\"iconfont icon-r-yes\" style=\"font-size: 22px;\"></i> 确认添加</el-button\r\n                >\r\n            </span>\r\n        </el-dialog>\r\n\r\n        <el-dialog\r\n            title=\"用户充值\"\r\n            :visible.sync=\"feeUserShow\"\r\n            width=\"50%\"\r\n            center\r\n            @close=\"feeUserShow = false\"\r\n        >\r\n            <el-form\r\n                :model=\"feeUser\"\r\n                :rules=\"feeUserRules\"\r\n                ref=\"feeUserRules\"\r\n                label-width=\"80px\"\r\n            >\r\n                <el-form-item label=\"用户名\" prop=\"username\">\r\n                    <el-input v-model=\"feeUser.username\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"金额\" prop=\"money\">\r\n                    <el-input v-model=\"feeUser.money\"></el-input>\r\n                </el-form-item>\r\n            </el-form>\r\n\r\n            <!-- 底部区域 -->\r\n            <span slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"feeUserShow = false\" style=\"font-size: 18px;\"><i class=\"iconfont icon-r-left\" style=\"font-size: 22px;\"></i> 取消</el-button>\r\n                <el-button type=\"primary\" @click=\"feeUserCon('feeUserRules')\"\r\n                    >确认充值</el-button\r\n                >\r\n            </span>\r\n        </el-dialog>\r\n    </div>\r\n</template>\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n    data() {\r\n        var checkUsername = (rule, value, callback) => {\r\n            if (!value) {\r\n                return callback(new Error(\"用户名不能为空\"));\r\n            }\r\n            setTimeout(() => {\r\n                axios\r\n                    .get(\"/api/user/checkUsername?username=\" + value)\r\n                    .then((res) => {\r\n                        if (!res.data.data) {\r\n                            callback(new Error(\"用户名已存在\"));\r\n                        } else {\r\n                            callback();\r\n                        }\r\n                    });\r\n            }, 10);\r\n        };\r\n        var checkAge = (rule, value, callback) => {\r\n            if (!value) {\r\n                return callback(new Error(\"年龄不能为空\"));\r\n            }\r\n            setTimeout(() => {\r\n                if (!Number.isInteger(value)) {\r\n                    callback(new Error(\"请输入数字值\"));\r\n                } else {\r\n                    if (value < 18) {\r\n                        callback(new Error(\"必须年满18岁\"));\r\n                    } else {\r\n                        callback();\r\n                    }\r\n                }\r\n            }, 1000);\r\n        };\r\n        return {\r\n            total: 0,\r\n            loading: true,\r\n            queryInfo: {\r\n                username: \"\",\r\n                nike: \"\",\r\n                card: \"\",\r\n                pagenum: 1,\r\n                pageSize: 6,\r\n            },\r\n            users: [],\r\n            editUser: {\r\n                password: \"\",\r\n                nike: \"\",\r\n                sex: \"\",\r\n                age: \"\",\r\n                phone: \"\",\r\n                money: \"\",\r\n                card: \"\",\r\n            },\r\n            registerUser: {\r\n                sex: \"男\",\r\n            },\r\n            addShow: false,\r\n            editShow: false,\r\n            editRules: {\r\n                niek: [\r\n                    { required: true, message: \"请输入姓名\", trigger: \"blur\" },\r\n                ],\r\n                password: [\r\n                    { required: true, message: \"请输入密码\", trigger: \"blur\" },\r\n                    {\r\n                        min: 6,\r\n                        max: 15,\r\n                        message: \"长度在 6 到 15 个字符\",\r\n                        trigger: \"blur\",\r\n                    },\r\n                ],\r\n                age: [{ validator: checkAge, trigger: \"blur\" }],\r\n                phone: [\r\n                    {\r\n                        required: true,\r\n                        message: \"请输入联系电话\",\r\n                        trigger: \"blur\",\r\n                    },\r\n                ],\r\n                card: [\r\n                    {\r\n                        required: true,\r\n                        message: \"请输入车牌号\",\r\n                        trigger: \"blur\",\r\n                    },\r\n                ],\r\n            },\r\n            registerRules: {\r\n                username: [{ validator: checkUsername, trigger: \"blur\" }],\r\n                password: [\r\n                    { required: true, message: \"请输入密码\", trigger: \"blur\" },\r\n                    {\r\n                        min: 6,\r\n                        max: 15,\r\n                        message: \"长度在 6 到 15 个字符\",\r\n                        trigger: \"blur\",\r\n                    },\r\n                ],\r\n                niek: [\r\n                    { required: true, message: \"请输入姓名\", trigger: \"blur\" },\r\n                ],\r\n                age: [{ validator: checkAge, trigger: \"blur\" }],\r\n                phone: [\r\n                    {\r\n                        required: true,\r\n                        message: \"请输入联系电话\",\r\n                        trigger: \"blur\",\r\n                    },\r\n                ],\r\n                card: [\r\n                    {\r\n                        required: true,\r\n                        message: \"请输入车牌号\",\r\n                        trigger: \"blur\",\r\n                    },\r\n                ],\r\n            },\r\n            feeUserShow: false,\r\n            feeUserRules: {\r\n                username: [\r\n                    {\r\n                        required: true,\r\n                        message: \"请输入用户名\",\r\n                        trigger: \"blur\",\r\n                    },\r\n                ],\r\n                money: [\r\n                    {\r\n                        required: true,\r\n                        message: \"请输入充值金额\",\r\n                        trigger: \"blur\",\r\n                    },\r\n                ],\r\n            },\r\n            feeUser: {\r\n                username: \"\",\r\n                money: \"\",\r\n            },\r\n        };\r\n    },\r\n    methods: {\r\n        feeUserCon(formName) {\r\n            // 充值\r\n            this.$refs[formName].validate((valid) => {\r\n                if (valid) {\r\n                    axios.post(\"/api/fee/userFee\", this.feeUser).then((res) => {\r\n                        if (res.data.data.flag) {\r\n                            this.$message.success(\"充值成功！\");\r\n                            this.feeUserShow = false;\r\n                            this.getList();\r\n                            this.feeUser = {\r\n                                username: \"\",\r\n                                money: \"\",\r\n                            };\r\n                        } else {\r\n                            this.$message.error(res.data.data.msg);\r\n                        }\r\n                    });\r\n                } else {\r\n                    return false;\r\n                }\r\n            });\r\n        },\r\n        getList() {\r\n            axios.post(\"/api/user/getUsers\", this.queryInfo).then((res) => {\r\n                this.loading = false;\r\n                this.total = res.data.data.total;\r\n                this.users = res.data.data.records;\r\n            });\r\n        },\r\n        handleSizeChange(newSize) {\r\n            this.queryInfo.pageSize = newSize;\r\n            this.getList();\r\n        },\r\n        handleCurrentChange(newPage) {\r\n            this.queryInfo.pagenum = newPage;\r\n            this.getList();\r\n        },\r\n        addUser() {\r\n            for (let item in this.registerUser) {\r\n                this.registerUser[item] = \"\";\r\n            }\r\n            this.registerUser.sex = \"男\";\r\n            this.addShow = true;\r\n        },\r\n        editClose() {\r\n            this.editShow = false;\r\n            this.$nextTick(() => {\r\n                this.$refs.editRules.resetFields();\r\n            });\r\n        },\r\n        toEdit(user) {\r\n            this.editUser.uid = user.uid;\r\n            this.editUser.password = user.password;\r\n            this.editUser.nike = user.nike;\r\n            this.editUser.sex = user.sex;\r\n            this.editUser.age = user.age;\r\n            this.editUser.phone = user.phone;\r\n            this.editUser.money = user.money;\r\n            this.editUser.card = user.card;\r\n\r\n            this.editShow = true;\r\n        },\r\n        editCommit(formName) {\r\n            this.$refs[formName].validate((valid) => {\r\n                if (valid) {\r\n                    //日期进行转换\r\n                    // this.editUser.birthday=new Date(this.editUser.birthday).getTime();\r\n                    //请求\r\n                    axios\r\n                        .post(\"/api/user/update\", this.editUser)\r\n                        .then((res) => {\r\n                            if (res.data.flag) {\r\n                                this.$message.success(\"修改成功！\");\r\n                                this.getList();\r\n                            } else {\r\n                                this.$message.error(\"修改失败！\");\r\n                            }\r\n                            this.editShow = false;\r\n                        });\r\n                } else {\r\n                    return false;\r\n                }\r\n            });\r\n        },\r\n        async userDelete(uid) {\r\n            const re = await this.$confirm(\r\n                \"此操作将永久删除该用户, 是否继续?\",\r\n                \"提示\",\r\n                {\r\n                    confirmButtonText: \"确定\",\r\n                    cancelButtonText: \"取消\",\r\n                    type: \"warning\",\r\n                }\r\n            ).catch((err) => err);\r\n            // console.log(re)\r\n            if (re != \"confirm\") {\r\n                this.$message.info(\"已取消删除！\");\r\n            } else {\r\n                axios.get(\"/api/user/del?uid=\" + uid).then((res) => {\r\n                    if (res.data.data) {\r\n                        this.$message.success(\"删除成功！\");\r\n                        this.getList();\r\n                    } else {\r\n                        this.$message.error(\"删除失败！\");\r\n                    }\r\n                });\r\n            }\r\n        },\r\n        async rePassword(uid) {\r\n            const re = await this.$confirm(\r\n 